
<table class="table table-bordered">
	<tbody>
			<tr>
		  <th>Name:</th>
		  <td><%= @customer.name %></td>
		</tr>
			<tr>
		  <th>Clienttype:</th>
		  <td><%= @customer.clientType %></td>
		</tr>
			<tr>
		  <th>Status:</th>
		  <td><%= @customer.status %></td>
		</tr>
			<tr>
		  <th>Rut:</th>
		  <td><%= @customer.rut %></td>
		</tr>
			<tr>
		  <th>Email:</th>
		  <td><%= @customer.email %></td>
		</tr>
		</tbody>
</table>

<%= link_to 'Edit', edit_customer_path(@customer), :class => "btn btn-default" %>
<%= link_to 'Destroy', @customer, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %>
<%= link_to 'Back', customers_path, :class => "btn btn-link" %>
